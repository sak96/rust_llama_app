{
  "build": {
    "beforeDevCommand": "trunk serve",
    "beforeBuildCommand": "trunk build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "rust_llama_app",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
        "dialog": {
          "all": true,
          "open": true
      },
      "fs": {
        "all": false,
        "readFile": true,
        "exists": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.dev",
      "icon": [
        "icons/icon.png"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "rust_llama_app",
        "width": 800,
        "height": 600
      }
    ]
  }
}
